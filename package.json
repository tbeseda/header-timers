{
  "name": "header-timers",
  "version": "0.1.1",
  "description": "Server-Timing HTTP header helper",
  "author": "tbeseda",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbeseda/header-timers.git"
  },
  "keywords": [
    "http",
    "server-timing",
    "header",
    "timer"
  ],
  "main": "index.js",
