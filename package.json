    "lint.fix": "npm run lint -- --fix",
    "test": "npm run lint && node --test"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "ignorePatterns": [
      "src/vendor"
    ],
    "rules": {
      "space-unary-ops": 0,
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
